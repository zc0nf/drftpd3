log4j.logger.org.drftpd.commands.tvmaze=WARN, tvmaze
log4j.additivity.org.drftpd.commands.tvmaze=false

log4j.appender.tvmaze=org.apache.log4j.DailyRollingFileAppender
log4j.appender.tvmaze.File=logs/tvmaze.log
log4j.appender.tvmaze.layout=org.apache.log4j.PatternLayout
log4j.appender.tvmaze.layout.ConversionPattern=%p %d{ISO8601} [%t] %c %x - %m%n