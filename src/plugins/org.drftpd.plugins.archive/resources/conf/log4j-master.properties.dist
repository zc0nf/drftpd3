# $Id$
log4j.logger.org.drftpd.plugins.archive=ERROR, archive
log4j.additivity.org.drftpd.plugins.archive=false

log4j.appender.archive=org.apache.log4j.DailyRollingFileAppender
log4j.appender.archive.File=logs/archive.log
log4j.appender.archive.layout=org.apache.log4j.PatternLayout
log4j.appender.archive.layout.ConversionPattern=%p %d{ISO8601} [%t] %c %x - %m%n